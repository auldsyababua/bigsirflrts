{
  "name": "telegram-webhook-handler",
  "version": "2.0.0",
  "description": "Telegram webhook handler with OpenAI and ERPNext integration for BigSirFLRTS bot",
  "type": "module",
  "main": "index.mjs",
  "scripts": {
    "test": "vitest run",
    "test:unit": "vitest run tests/*.test.js --exclude tests/integration.test.js --exclude tests/e2e-complete-flow.test.js --reporter=verbose",
    "test:integration": "vitest run tests/integration.test.js --reporter=verbose",
    "test:e2e": "vitest run tests/e2e-complete-flow.test.js --reporter=verbose",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:smoke": "./tests/smoke-test.sh",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:e2e:watch": "vitest watch tests/e2e-complete-flow.test.js"
  },
  "keywords": [
    "telegram",
    "webhook",
    "lambda",
    "openai",
    "erpnext",
    "nlp"
  ],
  "author": "10netzero",
  "license": "UNLICENSED",
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "openai": "^4.73.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "vitest": "^3.2.4"
  }
}
