{
  "name": "telegram-webhook-handler",
  "version": "2.0.0",
  "description": "Telegram webhook handler with OpenAI and ERPNext integration for BigSirFLRTS bot",
  "type": "module",
  "main": "index.mjs",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --exclude tests/integration.test.js",
    "test:erpnext": "vitest run tests/erpnext.test.js --reporter=verbose",
    "test:integration": "vitest run tests/integration.test.js --reporter=verbose",
    "test:all": "vitest run --reporter=verbose"
  },
  "keywords": [
    "telegram",
    "webhook",
    "lambda",
    "openai",
    "erpnext",
    "nlp"
  ],
  "author": "10netzero",
  "license": "UNLICENSED",
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "openai": "^4.73.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "vitest": "^3.2.4"
  }
}
